{
  "name": "digbiz5",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__.*\\.test\\.(ts|tsx|js)$ --testPathIgnorePatterns=integration|components|e2e",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:components": "jest --testPathPattern=__tests__/components",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "test:setup": "node _tests_/scripts/runIndividualTest.js setup",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:components",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "verify": "npm run typecheck && npm run lint && npm run test:setup",
    "build:android": "cd android && ./gradlew assembleDebug",
    "build:ios": "cd ios && xcodebuild -workspace digbiz5.xcworkspace -scheme digbiz5 -configuration Debug -destination 'platform=iOS Simulator,name=iPhone 15' build",
    "prepare": "husky",
    "test:phase2": "node _tests_/scripts/runPhase2Tests.js",
    "test:phase2:verbose": "node _tests_/scripts/runPhase2Tests.js --verbose",
    "test:phase2:coverage": "node _tests_/scripts/runPhase2Tests.js --coverage",
    "test:card-creation": "node _tests_/scripts/runIndividualTest.js cardCreation",
    "test:qr-code": "node _tests_/scripts/runIndividualTest.js qrCode",
    "test:deep-link": "node _tests_/scripts/runIndividualTest.js deepLink",
    "test:card-preview": "node _tests_/scripts/runIndividualTest.js preview",
    "test:sharing": "node _tests_/scripts/runIndividualTest.js sharing",
    "test:wallet": "node _tests_/scripts/runIndividualTest.js wallet",
    "test:businesscard-integration": "node _tests_/scripts/runIndividualTest.js integration",
    "test:phase1": "node _tests_/scripts/runPhase1Tests.js",
    "test:phase1:verbose": "node _tests_/scripts/runPhase1Tests.js --verbose",
    "test:phase1:coverage": "node _tests_/scripts/runPhase1Tests.js --coverage",
    "test:typescript": "node _tests_/scripts/runIndividualTest.js typescript",
    "test:linting": "node _tests_/scripts/runIndividualTest.js linting",
    "test:auth": "node _tests_/scripts/runIndividualTest.js authService",
    "test:google-auth": "node _tests_/scripts/runIndividualTest.js googleAuth",
    "test:linkedin-auth": "node _tests_/scripts/runIndividualTest.js linkedinAuth",
    "test:profile": "node _tests_/scripts/runIndividualTest.js profileService",
    "test:profile-validation": "node _tests_/scripts/runIndividualTest.js profileValidation",
    "test:tokens": "node _tests_/scripts/runIndividualTest.js tokenUtils",
    "test:all-phases": "node _tests_/scripts/runIndividualTest.js all",
    "test:networking": "node _tests_/scripts/runNetworkingTests.js",
    "test:networking:quick": "node _tests_/scripts/runNetworkingTests.js --skip-load-tests",
    "test:networking:full": "node _tests_/scripts/runNetworkingTests.js --include-benchmarks",
    "test:search-algorithm": "jest __tests__/networking/searchAlgorithm.test.ts",
    "test:connection-states": "jest __tests__/networking/connectionStates.test.ts",
    "test:message-delivery": "jest __tests__/networking/messageDelivery.test.ts",
    "test:push-notifications": "jest __tests__/networking/pushNotifications.test.ts",
    "test:realtime-sync": "jest __tests__/networking/realTimeSync.test.ts",
    "test:discovery-load": "jest __tests__/networking/discoveryLoadTests.test.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-native-ml-kit/text-recognition": "^2.0.0",
    "@react-native/new-app-screen": "0.81.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/drawer": "^7.5.8",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@reduxjs/toolkit": "^2.9.0",
    "crypto-js": "^4.2.0",
    "react": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.81.1",
    "react-native-background-job": "^2.3.1",
    "react-native-config": "^1.5.9",
    "react-native-contacts": "^8.0.7",
    "react-native-document-picker": "^9.3.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-html-to-pdf": "^1.3.0",
    "react-native-image-crop-picker": "^0.51.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-linkedin": "^2.1.1",
    "react-native-mmkv": "^3.3.1",
    "react-native-paper": "^5.14.5",
    "react-native-print": "^0.11.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "^4.1.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.16.0",
    "react-native-share": "^12.2.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "^15.13.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-view-shot": "^4.0.3",
    "react-native-vision-camera": "^4.7.2",
    "react-native-webview": "^13.16.0",
    "react-redux": "^9.2.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.1",
    "@react-native/eslint-config": "0.81.1",
    "@react-native/metro-config": "0.81.1",
    "@react-native/typescript-config": "0.81.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "detox": "^20.41.1",
    "eslint": "^8.19.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
